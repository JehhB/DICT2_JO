<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>
      {% block title %}
      DICT Job Order System
      {% endblock %}
    </title>
    <link
      rel="icon"
      href="{{ asset('assets/dict-logo.png') }}"
    >
    {% block head %}
    {% endblock %}
    {{ encore_entry_script_tags('app') }}
    {{ encore_entry_link_tags('app') }}
  </head>
  <html>
    <body>
      <header>
        {% block header %}
        {% endblock %}
      </header>
      <main>
        {% block body %}
        {% endblock %}
      </main>
    </body>
  </html>
</html>
